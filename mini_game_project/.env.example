# Example environment variables for the mini_game_project.

# SECRET_KEY is used by Django for cryptographic signing. Generate your own secret key
# and keep it safe. You can generate one with ``python -c 'from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())'``.
DJANGO_SECRET_KEY=change-me

# Turn off debug mode in production by setting this to ``False``.
DJANGO_DEBUG=True

# Comma-separated list of hosts/domain names that are valid for this site. For local
# development you can leave this as-is. In production, set this to your domain(s).
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1